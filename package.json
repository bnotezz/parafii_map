{
  "name": "daro-metric-books",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "python3 scripts/export_parafii_to_tree_view.py && python3 scripts/export_parafii_to_geojson.py && node scripts/copy-data.mjs && node scripts/generate-sitemap.mjs",
    "build": "next build",
    "preexport": "node scripts/copy-data.mjs",
    "start": "serve out",
    "lint": "next lint",
    "preview": "npm run pages:build && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "npm run pages:build && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "npm run pages:build && opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@next/third-parties": "latest",
    "@opennextjs/cloudflare": "^1.14.7",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-slot": "latest",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fs": "latest",
    "fuse.js": "latest",
    "iconv-lite": "^0.7.1",
    "lucide-react": "^0.562.0",
    "module": "^2.0.0",
    "next": "^16.1.1",
    "next-themes": "latest",
    "path": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "url": "latest"
  },
  "devDependencies": {
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10",
    "eslint": "^9.39",
    "eslint-config-next": "latest",
    "postcss": "^8.5",
    "serve": "^14.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": "22.x"
  }
}
